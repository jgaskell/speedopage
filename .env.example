# SpeedoPage Environment Configuration
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3101
NODE_ENV=production

# PostgreSQL Database (app1 as per DB_Setups.md)
DB_URL=postgres://app1:StrongPass1@localhost:5432/app1

# Redis Configuration (database 0 for app1)
REDIS_URL=redis://:yourRedisPassword@localhost:6379/0

# JWT Secret (generate a random string)
JWT_SECRET=your-secret-key-change-this-in-production

# Session Configuration
SESSION_SECRET=your-session-secret-change-this-in-production
